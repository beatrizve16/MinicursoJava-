package aula4;
import java.util.Scanner;
public class Ex2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite um numero inteiro");
		int n = sc.nextInt();
	int i;
		for (i =1; i<n; i++) {
			
			if (i % 2 != 0) {
				System.out.printf("%d\n", i);
			}
		}
		
	}

}
