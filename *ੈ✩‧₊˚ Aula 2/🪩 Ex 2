package aulaa3;
import java.util.Scanner;
public class Main1 {
	public static void main(String[] args) {
		int x;
		System.out.println("Digite o valor de X: ");
		Scanner sc = new Scanner(System.in);
		x = sc.nextInt();
		if ( x!=10) {
			 x=10;
			System.out.printf("%d" , x);
			
		}else if ( x==10) {
			
			System.out.printf(" %d" , x);
			
		}
	}
}
