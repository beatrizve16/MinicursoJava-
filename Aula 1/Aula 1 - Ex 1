package aula2;
import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
				  int a , b , c , d , diferenca;
			       
			        System.out.println("Digite os valores de A, B, C e D");
			        Scanner sc = new Scanner(System.in);
			       
			        a=sc.nextInt();
			        b=sc.nextInt();
			        c=sc.nextInt();
			        d=sc.nextInt();
			       
			        diferenca = ((a*b) - (c*d)) ;
			       
			        System.out.println("Diferenca =" + diferenca);
			}
	}
